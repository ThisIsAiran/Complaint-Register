		<select name ="hostel" id="hostel_name">
			  <option  value="Udaigiri">Udaigiri</option>
			  <option  value="Himgiri">Himgiri</option>
			  <option  value="Himadri">Himadri</option>
			  <option  value="Parvati">Parvati</option>
		</select>
		<input type="text" name="roomNo" placeholder="RoomNo" required>
		<input type="text" name="rollNo" placeholder="Roll No" required>
		<input type="submit" value="Sign Up" name="register">
	</form>
</div>
